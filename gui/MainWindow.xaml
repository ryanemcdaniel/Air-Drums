<Window x:Class="gui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:gui"
        mc:Ignorable="d"
        Title="MIDI Selection" Height="450" Width="800">

        <StackPanel>
  <Grid Margin="10,80,10,10" >
    <TextBox TextWrapping="Wrap"/>
  </Grid>

  <StackPanel Grid.Column="5" Grid.Row="0" HorizontalAlignment="Center" Name="ButtonPanel" VerticalAlignment="Top">
    <Button Margin="0,10,0,10">Dog Friendly</Button>
   
  </StackPanel>
</StackPanel>
    
</Window>


